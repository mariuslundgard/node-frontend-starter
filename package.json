{
  "name": "node-hipster-starter",
  "version": "1.0.0",
  "description": "Real hipster starter repos don’t follow no trend (and don’t use no emojis).",
  "main": "src/server.js",
  "scripts": {
    "build": "npm run build-client && npm run build-server",
    "build-client": "cross-env NODE_ENV=production rollup --config .rollup/client.config.js && node scripts/hash-bundles.js",
    "build-server": "cross-env NODE_ENV=production rollup --config .rollup/server.config.js",
    "clean": "rimraf dist",
    "clean-install": "rimraf node_modules package-lock.json && npm install",
    "docker-build": "docker build -t mariuslundgard/node-hipster-starter .",
    "docker-run": "docker run --rm --publish 3000:3000 mariuslundgard/node-hipster-starter",
    "format": "prettier-eslint --write \"{.rollup,scripts,src}/**/*.js\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "prewatch": "npm run clean",
    "start": "node scripts/start",
    "test": "jest",
    "typecheck": "flow check",
    "watch": "node scripts/dev"
  },
  "author": "Marius Lundgård <studio@mariuslundgard.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariuslundgard/node-hipster-starter.git"
  },
  "bugs": {
    "url": "https://github.com/mariuslundgard/node-hipster-starter/issues"
  },
  "homepage": "https://github.com/mariuslundgard/node-hipster-starter#readme",
  "engineStrict": true,
  "engines": {
    "node": "9.11.1"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.2",
    "babel-eslint": "^8.2.6",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chokidar": "^2.0.4",
    "cross-env": "^5.2.0",
    "cssnano": "^4.0.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.70.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^11.1.0",
    "postcss-nesting": "^4.2.1",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "supertest": "^3.0.0"
  }
}
