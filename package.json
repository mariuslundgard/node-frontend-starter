{
  "name": "node-hipster-starter",
  "version": "1.0.0",
  "description": "Real hipster starter repos don’t follow no trend (and don’t use no emojis).",
  "main": "src/server.js",
  "scripts": {
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack --config .webpack/production.config.js --optimize-minimize",
    "build-server": "webpack --config .webpack/server.config.js",
    "clean": "rimraf build",
    "clean-install": "rimraf node_modules package-lock.json && npm install",
    "docker-build": "docker build -t mariuslundgard/node-hipster-starter .",
    "docker-run": "docker run --rm --publish 3000:3000 mariuslundgard/node-hipster-starter",
    "format": "prettier-eslint --write \"{.webpack,scripts,src}/**/*.js\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "npm run lint && npm run typecheck && npm test",
    "start": "node scripts/start",
    "test": "cross-env NODE_PATH=src/lib jest",
    "typecheck": "flow check",
    "watch": "cross-env NODE_PATH=src/lib node scripts/dev"
  },
  "author": "Marius Lundgård <studio@mariuslundgard.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariuslundgard/node-hipster-starter.git"
  },
  "bugs": {
    "url": "https://github.com/mariuslundgard/node-hipster-starter/issues"
  },
  "homepage": "https://github.com/mariuslundgard/node-hipster-starter#readme",
  "engineStrict": true,
  "engines": {
    "node": "9.5.0"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0",
    "source-map-support": "^0.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chokidar": "^2.0.1",
    "cross-env": "^5.1.3",
    "css-hot-loader": "^1.3.7",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.43.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.65.0",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "postcss-css-variables": "^0.8.0",
    "postcss-loader": "^2.1.0",
    "postcss-nesting": "^4.2.1",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "supertest": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-node-externals": "^1.6.0"
  }
}
